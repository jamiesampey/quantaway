{
  "name": "quantaway",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf client/build server/build",
    "compile:client": "webpack --config webpack.config.js",
    "compile:server": "webpack --config webpack.server-config.js",
    "compile": "npm run clean && npm run compile:client && npm run compile:server",
    "watch:client": "webpack-dev-server --hot --inline",
    "watch:server": "webpack --config webpack.server-config.js --env.watchServer",
    "watch": "npm run clean & npm-run-all --parallel watch:client watch:server",
    "lint": "eslint --ext js,jsx client server webpack.config.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "express": "^4.16.2",
    "fast-csv": "^2.4.1",
    "morgan": "^1.9.0",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "source-map-support": "^0.5.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
